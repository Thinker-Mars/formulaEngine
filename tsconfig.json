{
    "compilerOptions": {
        "allowUnusedLabels": false,
        "alwaysStrict": false,
        "baseUrl": ".",
        "allowSyntheticDefaultImports": true,
        "module": "ESNext",
        "noImplicitAny": false,
        "paths": {
            "src": ["./src"]
        },
        "target": "ES5",
        "outDir": "dist",
        "declaration": false,
        "removeComments": false,
        "lib": ["DOM","ESNext"],
        "sourceMap": false
    },
    "ts-node": {
          "require": ["tsconfig-paths/register"]
    }
}