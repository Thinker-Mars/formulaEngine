{
    "compilerOptions": {
        "allowUnusedLabels": false,
        "alwaysStrict": false,
        "baseUrl": ".",
        "allowSyntheticDefaultImports": true,
        "module": "ES2015",
        "noImplicitAny": false,
        "moduleResolution": "Node",
        "paths": {
            "src": ["./src"]
        },
        "target": "ESNext",
        "outDir": "dist",
        "declaration": true,
        "removeComments": true,
        "lib": ["DOM", "ES2016"],
    }

}